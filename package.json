{
  "name": "bus-tracker",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:dev": "cross-env NODE_ENV=development expo run:android",
    "android:release": "expo run:android --variant release",
    "ios": "expo run:ios",
    "ios:dev": "cross-env NODE_ENV=development expo run:ios",
    "ios:release": "expo run:ios --configuration Release",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c && rm -rf node_modules && npm install",
    "prebuild": "expo prebuild --clean",
    "build:android": "npm run prebuild && cd android && ./gradlew assembleRelease",
    "build:android:dev": "cross-env NODE_ENV=development npm run prebuild && cd android && ./gradlew assembleDebug",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:android:http": "expo build:android",
    "install:android": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "build:android:local": "cross-env NODE_ENV=production npm run prebuild && cd android && gradlew assembleRelease",
    "build-and-install:android": "npm run build:android:local && npm run install:android"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@maplibre/maplibre-react-native": "^10.2.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-maps": "~0.11.0",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "cross-env": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
